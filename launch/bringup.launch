<?xml version="1.0"?>
<launch>

    <arg name="name" default="read_write_py_node_id_1"/> 
    <arg name="set_position_topic" default="set_position"/> 
    <arg name="arm" default="arm_1"/>
    <arg name="id_number" default="1"/>
    
    
    <group ns="$(arg arm)"> 

        <node pkg="dynamixel_pkg" type="read_write_node.py" name="$(arg name)">
            <param name="DXL_ID" value="$(arg id_number)"/>            
            <remap from="/set_position" to="$(arg set_position_topic)"/>
        </node>


        
    </group>
    
    

</launch>